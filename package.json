{
  "name": "curio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cleandata": "rm -rf mdb/data/db/*",
    "mongod": "mkdir -p mdb/data/db mdb/log/ && killall -q mongod; sleep 2; mongod --dbpath mdb/data/db --logpath mdb/log/mongod.log --fork",
    "dev": "npm run mongod && next dev",
    "freshdev": "npm run cleandata && npm run mongod && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.28.1",
    "@vercel/blob": "^0.16.1",
    "leaflet": "^1.9.4",
    "mongodb": "^6.3.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "jest": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5"
  }
}
